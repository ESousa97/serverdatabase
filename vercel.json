{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "bash vercel-build.sh",
  "functions": {
    "api/index.js": {
      "includeFiles": "database.sqlite,models/**,node_modules/sqlite3/lib/binding/**/*"
    },
    "api/cardlist.js": {
      "memory": 1024,
      "includeFiles": "database.sqlite,models/**,node_modules/sqlite3/lib/binding/**/*"
    },
    "api/categories.js": {
      "memory": 1024,
      "includeFiles": "database.sqlite,models/**,node_modules/sqlite3/lib/binding/**/*"
    },
    "api/project.js": {
      "memory": 1024,
      "includeFiles": "database.sqlite,models/**,node_modules/sqlite3/lib/binding/**/*"
    },
    "api/search.js": {
      "memory": 1024,
      "includeFiles": "database.sqlite,models/**,node_modules/sqlite3/lib/binding/**/*"
    }
  },
  "rewrites": [
    {
      "source": "/api/search",
      "destination": "/api/search.js"
    },
    {
      "source": "/api/project/:id",
      "destination": "/api/project.js"
    },
    {
      "source": "/api/categories",
      "destination": "/api/categories.js"
    },
    {
      "source": "/api/cards",
      "destination": "/api/cardlist.js"
    }
  ]
}
