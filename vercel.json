{
  "version": 2,
  "outputDirectory": ".",
  "functions": {
    "api/index.js": {
      "includeFiles": "database.sqlite"
    },
    "api/cardlist.js": {
      "memory": 1024,
      "includeFiles": "models/**"
    },
    "api/categories.js": {
      "memory": 1024,
      "includeFiles": "models/**"
    },
    "api/project.js": {
      "memory": 1024,
      "includeFiles": "models/**"
    },
    "api/search.js": {
      "memory": 1024,
      "includeFiles": "models/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
