{
  "version": 2,
  "outputDirectory": ".",
  "functions": {
    "api/index.js": {
      "includeFiles": "database.sqlite"
    },
    "api/cardlist.js": {
      "memory": 1024,
      "includeFiles": "models/**"
    },
    "api/categories.js": {
      "memory": 1024,
      "includeFiles": "models/**"
    },
    "api/project.js": {
      "memory": 1024,
      "includeFiles": "models/**"
    },
    "api/search.js": {
      "memory": 1024,
      "includeFiles": "models/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/search",
      "destination": "/api/search.js"
    },
    {
      "source": "/api/project/:id",
      "destination": "/api/project.js"
    },
    {
      "source": "/api/categories",
      "destination": "/api/categories.js"
    },
    {
      "source": "/api/cards",
      "destination": "/api/cardlist.js"
    }
  ]
}
